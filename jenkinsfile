pipeline {
    agent any
    stages {
        stage ('welcome'){
            steps {
                echo "I'm inside the pipeline"
            }
        }
        stage ('Build'){
            steps {
                script {
                    sh docker.build('dockerimage')
                }
            }
        }
        stage ('Run') {
            steps {
                script{
                   sh docker.run('dockerimage')
                }
            }
        }
            
            }
        }

        

